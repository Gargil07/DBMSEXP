CREATE TABLE STUDENTS(S_ID Varchar(5), 
S_NAME VARCHAR(20), 
S_ADDRESS VARCHAR(20), 
S_PHONE BIGINT, 
AGE INT); 

INSERT INTO STUDENTS VALUES('S1','AGNESH','DELHI',9455123451,18); 
INSERT INTO STUDENTS VALUES('S2','RAMESH','GURGAON',9655153451,18); 
INSERT INTO STUDENTS VALUES('S3','SUJIT','CHENNAI',9955113451,20); 
INSERT INTO STUDENTS VALUES('S4','SURESH','DELHI',9855123888,18); 
SELECT *FROM STUDENTS;

CREATE TABLE COURSE(C_ID Varchar(5),C_NAME VARCHAR(20)); 
INSERT INTO COURSE VALUES('C1','DSA'); 
INSERT INTO COURSE VALUES('C2','C++'); 
INSERT INTO COURSE VALUES('C3','DBMS'); 
SELECT *FROM COURSE;

CREATE TABLE STUDENT_COURSE(S_ID Varchar(5),C_ID Varchar(5)); 
INSERT INTO STUDENT_COURSE VALUES('S1','C1'); 
INSERT INTO STUDENT_COURSE VALUES('S1','C3'); 
INSERT INTO STUDENT_COURSE VALUES('S2','C1');

INSERT INTO STUDENT_COURSE VALUES('S3','C2'); 
INSERT INTO STUDENT_COURSE VALUES('S4','C2'); 
INSERT INTO STUDENT_COURSE VALUES('S4','C3'); 
SELECT *FROM STUDENT_COURSE; 

Select C_ID from COURSE where (C_NAME = 'DSA' or C_NAME = 'DBMS'); 
Select S_ID from STUDENT_COURSE where C_ID IN 

(SELECT C_ID from COURSE where C_NAME = 'DSA' or C_NAME = 'DBMS'); 
Select S_NAME from STUDENTS where S_ID IN 

(Select S_ID from STUDENT_COURSE where C_ID IN 
(SELECT C_ID from COURSE where C_NAME = 'DSA' or C_NAME = 'DBMS')); 

Select S_ID from STUDENTS where S_ID NOT IN 
(Select S_ID from STUDENT_COURSE where C_ID IN

(SELECT C_ID from COURSE where C_NAME = 'DSA' or C_NAME = 'DBMS')); 
